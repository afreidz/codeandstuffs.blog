{
  "name": "codeandstuffs.blog",
  "version": "0.0.1",
  "enginesStrict": true,
  "engines": {
    "node": "~14"
  },
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "prepare": "svelte-kit sync",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "vercel-build": "prisma generate && prisma migrate deploy && npm run build && npm run vercel-postbuild",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "vercel-postbuild": "cp node_modules/@prisma/engines/*query* .vercel_build_output/functions/node/render/ && cp prisma/schema.prisma .vercel_build_output/functions/node/render/"
  },
  "devDependencies": {
    "@fingerprintjs/fingerprintjs": "^3.3.3",
    "@fontsource/ibm-plex-mono": "^4.5.6",
    "@fontsource/ibm-plex-sans": "^4.5.7",
    "@fontsource/permanent-marker": "^4.5.3",
    "@fontsource/roboto-serif": "^4.5.4",
    "@prisma/client": "^3.12.0-dev.46",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "canvas-confetti": "^1.5.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "husky": "^7.0.4",
    "mdsvex": "^0.10.5",
    "p-map": "^5.3.0",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.0",
    "sass": "^1.49.9",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.10.1",
    "the-new-css-reset": "^1.5.1",
    "tslib": "^2.3.1",
    "typescript": "~4.6.2"
  },
  "type": "module"
}
